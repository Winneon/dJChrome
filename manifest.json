{
	"manifest_version": 2,
	"name": "dJChrome",
	"version": "1.0.0",
	"description": "A streaming client for the dJ of WCN.",
	"author": "Jesse Bryan",
	"browser_action": {
		"default_popup": "index.html",
		"default_icon": "img/icon_19.png"
	},
	"background": {
		"page": "background.html"
	},
	"permissions": [
		"storage",
		"contextMenus",
		"notifications"
	],
	"icons": {
		"128": "img/icon_128.png"
	},
	"web_accessible_resources": [
		"img/icon_19.png",
		"img/icon_128.png"
	],
	"content_security_policy": "script-src 'self' 'unsafe-eval' https://code.jquery.com; object-src 'self'"
}